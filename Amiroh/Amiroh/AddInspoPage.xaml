<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Amiroh.AddInspoPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="titleEntry" Placeholder="Title"></Entry>
            
            <Label></Label>
            <Label Text="Description"></Label>
            <Editor x:Name="descriptionEntry"></Editor>
            <Button Text="Choose Image" Clicked="AddImageButton_Clicked"></Button>
            <Button  x:Name= "btnUpload" Text = "Upload Inspo" Clicked="UploadButton_Clicked" IsEnabled="False"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>